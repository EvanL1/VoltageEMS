{
  "id": "meter_model",
  "name": "电表模型",
  "description": "电表实时状态监控和数据采集模型",
  "input_mappings": [
    {
      "source_key": "ems:data:meter",
      "source_field": "status",
      "target_field": "status",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "active_power",
      "target_field": "active_power",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "reactive_power",
      "target_field": "reactive_power",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "apparent_power",
      "target_field": "apparent_power",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "voltage_a",
      "target_field": "voltage_a",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "voltage_b",
      "target_field": "voltage_b",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "voltage_c",
      "target_field": "voltage_c",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "current_a",
      "target_field": "current_a",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "current_b",
      "target_field": "current_b",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "current_c",
      "target_field": "current_c",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "frequency",
      "target_field": "frequency",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "power_factor",
      "target_field": "power_factor",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "import_energy",
      "target_field": "import_energy",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "export_energy",
      "target_field": "export_energy",
      "transform": null
    },
    {
      "source_key": "ems:data:meter",
      "source_field": "peak_demand",
      "target_field": "peak_demand",
      "transform": null
    }
  ],
  "output_key": "ems:model:output:meter",
  "enabled": true,
  "actions": [
    {
      "id": "meter_voltage_high_alert",
      "name": "电压过高告警",
      "action_type": "RemoteControl",
      "channel": "Alarm_System",
      "point": "high_voltage_alarm",
      "value": "1",
      "conditions": [
        {
          "field": "voltage_a",
          "operator": ">",
          "value": "253"
        }
      ],
      "enabled": true
    },
    {
      "id": "meter_voltage_low_alert",
      "name": "电压过低告警",
      "action_type": "RemoteControl",
      "channel": "Alarm_System",
      "point": "low_voltage_alarm",
      "value": "1",
      "conditions": [
        {
          "field": "voltage_a",
          "operator": "<",
          "value": "207"
        }
      ],
      "enabled": true
    },
    {
      "id": "meter_frequency_alert",
      "name": "频率异常告警",
      "action_type": "RemoteControl",
      "channel": "Alarm_System",
      "point": "frequency_alarm",
      "value": "1",
      "conditions": [
        {
          "field": "frequency",
          "operator": "<",
          "value": "49.5"
        }
      ],
      "enabled": true
    },
    {
      "id": "meter_peak_demand_alert",
      "name": "需量超限告警",
      "action_type": "RemoteControl",
      "channel": "Alarm_System",
      "point": "peak_demand_alarm",
      "value": "1",
      "conditions": [
        {
          "field": "active_power",
          "operator": ">",
          "value": "9000"
        }
      ],
      "enabled": true
    },
    {
      "id": "meter_data_logging",
      "name": "电表数据记录",
      "action_type": "RemoteControl",
      "channel": "Data_Logger",
      "point": "log_meter_data",
      "value": "1",
      "conditions": [
        {
          "field": "status",
          "operator": "==",
          "value": "online"
        }
      ],
      "enabled": true
    }
  ]
} 